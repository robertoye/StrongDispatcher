<?xml version="1.0" encoding="utf-8" ?>
<!--
  runingstatus ：运行状态
  errormonitorinterval ： 错误监测线程运行时间间隔，以分钟为单位，未设置缺省为5分钟
  
  Mission:
    大小写敏感，全部为小写字母，
    name ：应用名称；
    location：dll文件地址，缺省在当前目录查询，可填写绝对路径，必须设置项；
    classname：类名称，必须包含namespace；必须设置项；
    lanchmethod：重复发起执行的方法名；必须设置项；
    shutdownmethod：服务停止时执行的方法名；必须设置项；
    lanchinterval：重复执行lanchMethod时间间隔；必须设置项；
    errortryinterval：发生错误后，再次执行的时间间隔，以分钟，必须设置项；
    missionstatus:状态，Running正常运行，Stop停止，缺省为Running
    desc:解释字段，可以不填
  --> 
<conf runingstatus="test">
  <running  errormonitorinterval="5">
    <Mission name="test1" missionstatus="Running1" location="test1.dll" classname="Test1.Class1" lanchmethod="Launch"  shutdownmethod="" lanchinterval="1" errortryinterval="60" desc="Test1" />
    <Mission name="test2" missionstatus="Stop" location="test2.dll" classname="Test2.Class1" lanchmethod="Launch"  shutdownmethod="" lanchinterval="1" errortryinterval="60" desc="Test2"  />
  </running>  
  <test errormonitorinterval="5">
    <Mission name="test1" missionstatus="Running" location="test1.dll" classname="Test1.Class1" lanchmethod="Launch"  shutdownmethod="" lanchinterval="1" errortryinterval="60" desc="Test1" />
    <Mission name="test2" missionstatus="Running" location="test2.dll" classname="Test2.Class1" lanchmethod="Launch"  shutdownmethod="" lanchinterval="1" errortryinterval="60" desc="Test2"  />
  </test>
</conf>